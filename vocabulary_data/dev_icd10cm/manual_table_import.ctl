OPTIONS (errors=0, direct=true, skip=1)
LOAD DATA 
INFILE 'manual_table_data.csv'
APPEND
INTO TABLE MANUAL_TABLE
FIELDS TERMINATED BY ','
OPTIONALLY ENCLOSED BY '"'
TRAILING NULLCOLS (
"CONCEPT_CODE_1"        CHAR,
"CONCEPT_NAME_1"        CHAR,
"VOCABULARY_ID_1"       CHAR,
"INVALID_REASON_1"      CHAR,
"CONCEPT_CODE_2"        CHAR,
"CONCEPT_NAME_2"        CHAR,
"CONCEPT_CLASS_ID_2"    CHAR,
"VOCABULARY_ID_2"       CHAR,
"INVALID_REASON_2"      CHAR,
"RELATIONSHIP_ID"       CHAR,
"VALID_START_DATE"      CHAR "TRUNC(TO_DATE(:VALID_START_DATE, 'YYYY-MM-DD HH24:MI:SS'))",
"VALID_END_DATE"        CHAR "TRUNC(TO_DATE(:VALID_END_DATE, 'YYYY-MM-DD HH24:MI:SS'))",
"INVALID_REASON"        CHAR
)